#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_glob-all@3.1.0@glob-all/bin/glob-all" "$@"
  ret=$?
else 
  node  "$basedir/../../../_glob-all@3.1.0@glob-all/bin/glob-all" "$@"
  ret=$?
fi
exit $ret
